<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="293.0" prefWidth="335.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.titleScreen.titleScreenController">
   <children>
      <ImageView fitHeight="293.0" fitWidth="335.0" layoutY="1.0">
         <image>
            <Image url="@../Images/blue_panel.png" />
         </image>
      </ImageView>
      <StackPane layoutX="73.0" layoutY="84.0" prefHeight="49.0" prefWidth="183.0">
         <children>
            <ImageView onKeyPressed="#startGame">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="PLAY!" textFill="#eaff00">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </StackPane>
      <StackPane layoutX="73.0" layoutY="167.0" onMouseClicked="#EndGame" prefHeight="49.0" prefWidth="190.0">
         <children>
            <ImageView>
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="QUIT" textFill="#eaff00">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="14.0" />
               </font>
            </Label>
         </children>
      </StackPane>
      <Label layoutX="51.0" layoutY="28.0" text="TRASH MASTER" textFill="#faff00">
         <font>
            <Font name="Gill Sans Ultra Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
