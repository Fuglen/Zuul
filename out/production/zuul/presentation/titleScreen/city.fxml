<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onMouseMoved="#setGameText" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.titleScreen.cityController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="980.0" onMouseMoved="#pointsText">
         <image>
            <Image url="@../Images/istockphoto-594482558-612x612.jpg" />
         </image>
      </ImageView>
      <StackPane layoutX="66.0" layoutY="397.0" onMouseClicked="#goWork" onMouseEntered="#hover" onMouseExited="#nonHover" prefHeight="29.0" prefWidth="89.0">
         <children>
            <ImageView fitHeight="38.0" fitWidth="93.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="Work" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <StackPane layoutX="369.0" layoutY="469.0" onMouseClicked="#goMcdonalds" onMouseEntered="#hover" onMouseExited="#nonHover" prefHeight="29.0" prefWidth="89.0">
         <children>
            <ImageView fitHeight="38.0" fitWidth="93.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label fx:id="mcdButton" text="Mcdonalds" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <StackPane layoutX="14.0" layoutY="539.0" onMouseClicked="#goRoad" onMouseEntered="#hover" onMouseExited="#nonHover" prefHeight="29.0" prefWidth="89.0">
         <children>
            <ImageView fitHeight="38.0" fitWidth="93.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="Road" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <StackPane layoutX="873.0" layoutY="539.0" onMouseClicked="#goPark" onMouseEntered="#hover" onMouseExited="#nonHover" prefHeight="29.0" prefWidth="89.0">
         <children>
            <ImageView fx:id="parkHomeButton" fitHeight="38.0" fitWidth="93.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="Park" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <TextArea fx:id="gameText" layoutX="363.0" layoutY="14.0" prefHeight="100.0" prefWidth="255.0" text="Room details" />
      <Label fx:id="points" layoutX="22.0" layoutY="14.0">
         <font>
            <Font name="Gill Sans Ultra Bold" size="14.0" />
         </font>
      </Label>
      <ImageView fx:id="NaomiKlein" fitHeight="82.0" fitWidth="63.0" layoutX="587.0" layoutY="428.0" onMouseClicked="#cityQuest">
         <image>
            <Image url="@../Images/Woman%20NPC.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
