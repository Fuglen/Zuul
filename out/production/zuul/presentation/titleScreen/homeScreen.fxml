<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane onMouseMoved="#loadSceneData" prefHeight="599.0" prefWidth="968.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.titleScreen.homeScreenController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="980.0" layoutY="-1.0">
         <image>
            <Image url="@../Images/homeImage.png" />
         </image>
      </ImageView>
      <StackPane fx:id="roadButton" layoutX="856.0" layoutY="545.0" onMouseClicked="#goRoad" onMouseEntered="#hover" onMouseExited="#hover" prefHeight="31.0" prefWidth="98.0">
         <children>
            <ImageView fitHeight="40.0" fitWidth="105.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="ROAD" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font></Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <ImageView fitHeight="63.0" fitWidth="53.0" layoutX="359.0" layoutY="359.0" onMouseClicked="#addItem">
         <image>
            <Image url="@../Images/bottle.png" />
         </image>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </ImageView>
      <ImageView layoutX="942.0" onMouseClicked="#EndGame">
         <image>
            <Image url="@../Images/green_boxCross.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
