<AnchorPane onMouseMoved="#setGameText" prefHeight="581.0" prefWidth="888.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.titleScreen.mcdonaldsController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="980.0" layoutX="1.0">
         <image>
            <Image url="@../Images/Mcdonalds.png" />
         </image>
      </ImageView>
      <StackPane fx:id="cityButton" layoutX="444.0" layoutY="551.0" onMouseClicked="#goCity" onMouseEntered="#hover" onMouseExited="#hover" prefHeight="23.0" prefWidth="83.0">
         <children>
            <ImageView fitHeight="35.0" fitWidth="95.0">
               <image>
                  <Image url="@../Images/green_button00.png" />
               </image>
            </ImageView>
            <Label text="City" textFill="#fff600">
               <font>
                  <Font name="Gill Sans Ultra Bold" size="12.0" />
               </font>
            </Label>
         </children>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </StackPane>
      <TextArea fx:id="gameText" layoutX="14.0" layoutY="486.0" prefHeight="100.0" prefWidth="255.0" text="Room details" />
   </children>
</AnchorPane>
